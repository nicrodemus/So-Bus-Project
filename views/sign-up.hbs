<section class="sign-upSection">
    <div class="sign-up-div">
        <h1>not a costumer yet? </h1>
        <h3>subscribe and get all the news </h3>
    </div>
    <div class="sign-up-structure">
        <form class="class-form" action="/process-signup" method="POST">

            <div class="div-form">
                <input class="input" type="text" name="preNom" placeholder="Prenom">
            </div>
            <div class="div-form">
                <input class="input" type="text" name="nom" placeholder="Nom">
            </div>
            <div class="div-form">
                <input class="input" type="text" name="eMail" placeholder="Email">
            </div>
            <div class="div-form">
                <input class="input" type="password" name="originalPassword" placeholder="Password">
            </div>
            <div class="button-sign">
                <button id="sign">Sign Up</button>
            </div>
        </form>
    </div>
</section>